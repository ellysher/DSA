   "source": [
    "class Solution:\n",
    "    def maxDepth(self, root: TreeNode) -> int:\n",
    "        if not root:\n",
    "            return 0\n",
    "\n",
    "        return 1 + max(\n",
    "            self.maxDepth(root.left),\n",
    "            self.maxDepth(root.right)\n",
    "        )\n"
   ]
